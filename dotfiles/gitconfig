[user]
	name = yourname
	email = yourmail@example.com
[color]
	ui = auto
[alias]
	st = status
	sts = status -s
	lo = log --oneline
	lost = log --stat
	ci = commit
	cia = commit -a --amend
	cim = commit -a -m
	br = branch
	brd = branch -d
	co = checkout
	cob = checkout -b
	df = diff
	dfs = diff --staged
	fl = log -u
	la = "!git config -l | grep alias"
	last = log -1 HEAD
	visual = !gitk
[pack]
	threads = 0
[rebase]
	autosquash = true
[core]
	editor = vim
